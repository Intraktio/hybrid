    "@ionic-native/core": "^3.8.0",
    "@ionic-native/push": "^3.8.0",
    "@ionic-native/splash-screen": "^3.8.0",
    "@ionic-native/status-bar": "^3.8.0",
    "@ionic-native/toast": "^3.8.0",
    "@ionic/storage": "2.0.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.2",
    "@ngx-translate/core": "^6.0.1",
    "@ngx-translate/http-loader": "^0.0.3",
    "angular2-moment": "^1.1.0",
    "clipboard": "^1.7.1",
    "debug": "^2.6.8",
    "deep-extend": "^0.4.1",
    "ionic-angular": "^3.8.0",
    "ionicons": "3.0.0",
    "lodash": "^4.17.3",
