    "rxjs": "^5.2.0",
    "sw-toolbox": "^3.4.0",
    "wp-api-angular": "Intraktio/wp-api-angular.git#dist",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/router": "^4.4.6",
    "@ionic-native/in-app-browser": "^3.8.0",
    "@ionic/app-scripts": "^3.1.8",
    "@types/jasmine": "2.5.41",
    "@types/node": "^7.0.22",
    "@types/prismjs": "^1.9.0",
    "@types/webpack-env": "^1.13.3",
    "angular2-template-loader": "^0.6.2",
    "cordova": "^7.1.0",
    "cson": "^4.1.0",
    "cson-loader": "^0.1.0",
    "css-loader": "^0.28.9",
    "deep-freeze": "^0.0.1",
    "file-loader": "^0.11.1",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "ionic": "^3.19.0",
    "jasmine": "^2.6.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
