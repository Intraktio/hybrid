    "style-loader": "^0.20.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.3.4",
    "webpack-merge": "^4.1.1"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "config": {
    "ionic_source_map_type": "eval",
    "ionic_webpack": "./build/config.js",
    "ionic_copy": "./build/copy.js",
    "ionic_watch": "./config/watch.config.js",
    "ionic_sass": "./build/sass.config.js"
  }
}
